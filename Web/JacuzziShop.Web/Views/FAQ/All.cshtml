@model JacuzziShop.Web.ViewModels.FAQ.FAQListViewModel
@{
    this.ViewData["Title"] = "All Questions";
}
<h1>@this.ViewData["Title"]</h1>
<hr/>

<div class="accordion" id="accordionExample">
    
    @foreach (var quesion in this.Model.Questions)
    {
        <p><strong>Question: @quesion.QuestionTitle</strong></p>
        <p><strong>Answer: </strong>@quesion.QuestionDescription</p>
        <hr/>
    }
</div>